    "#Given string to variable a\n",
    "a = \"I am a teacher and I love to inspire and teach people\"\n",
    "\n",
    "#Split the given string\n",
    "b = a.split()\n",
    "\n",
    "#Using set to remove duplicates\n",
    "set1 = set(b)\n",
    "\n",
    "#Counting number of words\n",
    "count = len(set1)\n",
    "\n",
    "#Print the result\n",
    "print(count)"
   ]
